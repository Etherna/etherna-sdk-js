{
  "name": "@etherna/sdk-js",
  "version": "2.0.0-beta2",
  "description": "Etherna SDKs for operations on the network",
  "author": "Mattia Dalzocchio",
  "license": "LGPL-3.0-or-later",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/etherna/etherna-sdk-js.git"
  },
  "keywords": [
    "etherna",
    "clients",
    "swarm",
    "bee",
    "index",
    "gateway",
    "sso",
    "schema",
    "manifest",
    "serializer",
    "deserializer"
  ],
  "bugs": {
    "url": "https://etherna.atlassian.net/projects/ESJ"
  },
  "homepage": "https://github.com/etherna/etherna-sdk-js#readme",
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "pnpm tsup src/index.ts --format esm --dts-resolve --clean --dts --watch",
    "build": "pnpm tsup src/index.ts --format esm --dts-resolve --minify --clean --sourcemap --dts",
    "test": "pnpm vitest",
    "test-ui": "pnpm vitest --ui",
    "lint": "pnpm eslint --ext .ts --fix .",
    "format": "pnpm prettier --write .",
    "typecheck": "pnpm tsc --noEmit"
  },
  "peerDependencies": {
    "@ffmpeg/ffmpeg": ">=0.12",
    "@ffmpeg/util": ">=0.12",
    "axios": ">=1.7"
  },
  "dependencies": {
    "@fairdatasociety/bmt-js": "2.1.0",
    "@ffmpeg/ffmpeg": "0.12.15",
    "@ffmpeg/util": "0.12.2",
    "@noble/hashes": "1.8.0",
    "@noble/secp256k1": "2.2.3",
    "axios": "1.9.0",
    "blurhash": "2.0.5",
    "crypto-ts": "1.0.2",
    "js-sha3": "0.9.3",
    "uuid-random": "1.3.2",
    "viem": "2.28.0",
    "zod": "3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "9.25.1",
    "@ianvs/prettier-plugin-sort-imports": "4.4.1",
    "@total-typescript/ts-reset": "0.6.1",
    "@types/node": "22.15.2",
    "eslint": "9.25.1",
    "eslint-config-prettier": "10.1.2",
    "eslint-plugin-prettier": "5.2.6",
    "globals": "16.0.0",
    "prettier": "3.5.3",
    "tsup": "8.4.0",
    "typescript": "5.8.3",
    "typescript-eslint": "8.31.0",
    "vite": "6.3.3",
    "vitest": "3.1.2"
  },
  "optionalDependencies": {
    "@vitest/ui": "1.5.0",
    "chokidar": "3.6.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": []
    }
